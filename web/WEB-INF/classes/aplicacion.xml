<?xml version = "1.0" encoding = "ISO-8859-1"?><!DOCTYPE aplicacion SYSTEM "dtds/aplicacion.dtd"><aplicacion nombre="G4GenWrapper"> 		<modulo-defecto>		<controlador nombre="Default" />	</modulo-defecto>		<modulo nombre="Inicio">        <modelo nombre="Anuncio"/>        <vista-web url="jsp/tablonAnuncios.jsp"/>        <controlador nombre="Anuncios"/>        <controlador-modo nombre="DetailOnly"/>    </modulo>        <modulo nombre="UsuarioAdmin">		<modelo nombre="Usuario"/>		<controlador nombre="usuario" />	</modulo>		<modulo nombre="Tercero">		<var-entorno nombre="XAVA_SEARCH_ACTION" valor="TerceroController.buscarTercero"/>		<modelo nombre="Tercero"/>		<controlador nombre="TerceroController" />	</modulo>		<modulo nombre="Provincia">		<modelo nombre="Provincia"/>		<controlador nombre="ProvinciaController" />	</modulo>		<modulo nombre="Municipio">		<var-entorno nombre="XAVA_SEARCH_ACTION" valor="MunicipioController.buscarMunicipio"/>		<modelo nombre="Municipio"/>		<controlador nombre="MunicipioController" />	</modulo>	</aplicacion>